{
  // Telegram bot API token
  "telegram_token": null,
  // Telegram bot owner ID,
  "owner": 0,
  // Update handler thread amount
  "threads": 4,
  // Plugins not to load
  "exclude_plugins": [],
  // Redis settings
  "redis_host": "localhost",
  "redis_port": 6379,
  "redis_db": 0,
  // Support URL
  "support_url": "https://example.com",
  // How error will be reported to user
  // Allowed values:
  // describe - shows error description to user. May leak data, dont use in production
  // none - doesnt show any sort of error description to user
  "error_report_type": "describe"
}